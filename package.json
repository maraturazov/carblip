    "@ngrx/entity": "^7.3.0",
    "@ngrx/store": "^7.3.0",
    "@ngrx/store-devtools": "^7.3.0",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "bootstrap": "^4.2.1",
    "bs-stepper": "^1.3.0",
    "core-js": "^2.6.5",
    "font-awesome": "^4.7.0",
    "hammerjs": "2.0.8",
